{
    "name": "phongtran/nails",
    "description": "",
    "keywords": [
        "phongtran",
        "nails"
    ],
    "homepage": "https://github.com/phongtran/nails",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Phong Tran & Dan Pham",
            "email": "phongtran.nvl@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "PhongTran\\Nails\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhongTran\\Nails\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhongTran\\Nails\\NailsServiceProvider"
            ],
            "aliases": {}
        }
    }
}
